func main(num) begin
    print "num = ", num
    print "num*3 = ", num*3
    print "num+5 = ", num+5
    print "num/5 = ", num/5
    print "4*(-num) = ", 4*(-num)
    print "num<<2 = ", num<<2
    print "num>>1 = ", num>>1
    print "(num-1)*(num+1)*(num+3) = ", (num-1)*(num+1)*(num+3)
end

