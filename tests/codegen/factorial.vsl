
func main(num) begin
    print "Factorial of ", num, " is: ", factorial(num)
end

func factorial(n) begin
    if n = 0 then
        return 1
    else
        return n * factorial(n - 1)
end


//TESTCASE: 0
//Factorial of 0 is: 1

