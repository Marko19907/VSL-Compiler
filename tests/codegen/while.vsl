func main() begin
    var i

    print "First printing every power of two less than 1000"

    i := 1
    while i < 1000 do begin
        print "i is now ", i
        i := i*2
    end

    print "Now printing every product with factors less than 6"

    i := 1
    while i < 6 do begin
        var j
        j := 1
        while j < i do begin
            print j, " * ", i, " = ", i*j
            j := j+1
        end
        i := i+1
    end
end

