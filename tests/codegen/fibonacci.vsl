
func main() begin
    var i, num
    num := 20
    i := 0
    while i < num do begin
        print "Fibonacci ", i, ": ", fibonacci(i)
        i := i + 1
    end
end

func fibonacci(n) begin
    if n = 0 then
        return 0
    else if n = 1 then
        return 1
    else
        return fibonacci(n - 1) + fibonacci(n - 2)
end


