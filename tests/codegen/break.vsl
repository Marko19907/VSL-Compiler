
func main() begin
    var x, y
    while 1=1 do begin
        y := x
        while 1=1 do begin
            print y, " <= ", x
            y := y - 1
            if y < 0 then break
        end

        print "===="

        x := x + 1
        if x=5 then break
    end
end

